:- eval(databasetest_01()).

databasetest_01() :-
	println(["==========================databasetest_01=========================="]),
	location(database,testdb,"jdbc:hsqldb:mem:bikestore","sa",""),
	dbopen(testdb,DB),
	issue_query(DB).

issue_query(DB) :-
	sql_select(DB,bikes,[[manufacturer,Man],[model,Mod]]),
	println([Man,": ",Mod]).